{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "react",
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": "./",
		"strict": true,
		"paths": {
			"@/*": ["src/*"]
		},
		"allowSyntheticDefaultImports": true
	}
}